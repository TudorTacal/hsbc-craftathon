<h1>New Employee</h1>

<div class="text-center">
  <%= form_for Project.new do |form| %>
    <%= form.text_field :title, :required => true %>
    <%= form.text_area :description,  :required => true %>
    <%= form.submit %>
  <% end %>
</div>
<div class=".btn-default">
  <%= link_to 'Back', employees_path %>
</div>
